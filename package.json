    "start": "next start",
    "export": "next export",
    "lint": "eslint ./components/** ./pages/** -c .eslintrc.json --fix --ext js,jsx"
  },
  "dependencies": {
    "next": "9.4.4",
    "react": "16.13.1",
    "react-dom": "16.13.1"
  },
  "devDependencies": {
